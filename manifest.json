{
  "applications": {
    "gecko": {
      "id": "m-www@black.rabbit",
      "strict_min_version": "52.0"
    }
  },

  "description": "m <-> www",
  "manifest_version": 2,
  "name": "m <-> www",
  "version": "0.1",
  "homepage_url": "https://github.com/lbrabbit/m-www",
  "icons": {
    "48": "icons/page-48.png"
  },

  "permissions": [
    "tabs"
  ],

  "background": {
    "scripts": ["m-www.js"]
  },

  "browser_action": {
    "browser_style": true,
    "default_icon": "icons/page-32.png"
  }

}
