{

  "description": "m <-> www",
  "manifest_version": 2,
  "name": "m <-> www",
  "version": "0.1",
  "homepage_url": "https://github.com/mdn/webextensions-examples/tree/master/open-my-page-button",
  "icons": {
    "48": "icons/page-48.png"
  },

  "permissions": [
    "tabs"
  ],

  "background": {
    "scripts": ["m-www.js"]
  },

  "browser_action": {
    "browser_style": true,
    "default_icon": "icons/page-32.png"
  }

}
